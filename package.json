{
  "name": "gamebet",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:watch": "jest --watch --passWithNoTests --runInBand",
    "load:test:env": "dotenv -e .env.test",
    "test:seed": "npm run test:load-envs prisma db seed",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "test": "dotenv -e .env.test jest -- -i",
    "test:cov": "npm run load:test:env jest -- --coverage --runInBand -i",
    "pull": "npx prisma db pull",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "Darlan Schwartz",
  "license": "ISC",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@prisma/client": "^5.5.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.0",
    "joi": "^17.9.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/cors": "^2.8.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/node": "^18.17.16",
    "@types/supertest": "^2.0.16",
    "dotenv-cli": "^7.3.0",
    "dotenv-expand": "^10.0.0",
    "faker": "^6.6.6",
    "nodemon": "^2.0.22",
    "prisma": "^5.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.2.2",
    "typescript-transform-paths": "^2.2.4"
  }
}
